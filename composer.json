{
    "name": "noscosystems/yii2-collection",
    "description": "Basic collection library",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "yii2 collection",
        "collection"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Chepurnoi",
            "email": "chepurnoi.igor@gmail.com"
        },
        {
            "name": "Puttiong Ander Vidal",
            "email": "puttiong@nosco.net"
        }
    ],
    "require": {
        "php": "^8.2",
        "yiisoft/yii2": "^2.0.53",
        "noscosystems/yii2-helpers": "^1.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "yii2mod\\collection\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/noscosystems/yii2-helpers.git"
        }
    ]
}
